local gui = Instance.new("ScreenGui")
local SkibidiMain = Instance.new("Frame")
local SkibidiTitle = Instance.new("TextLabel")
local TextBax = Instance.new("TextBox")
local chat = Instance.new("TextButton")

-- Set up the main ScreenGui
gui.Name = "SkibidiGui"
gui.Parent = game:GetService("CoreGui") -- You can change this if you want the GUI elsewhere
SkibidiMain.Size = UDim2.new(0.5, 0, 0.5, 0)
SkibidiMain.Position = UDim2.new(0.25, 0, 0.25, 0)
SkibidiMain.BackgroundColor3 = Color3.new(1, 1, 1)
SkibidiMain.BorderColor3 = Color3.new(0, 0, 0)
SkibidiMain.BorderSizePixel = 1
SkibidiMain.Active = true
SkibidiMain.Draggable = true
SkibidiMain.Parent = gui

-- Set up the title of the GUI
SkibidiTitle.Size = UDim2.new(1, 0, 0.2, 0)
SkibidiTitle.Position = UDim2.new(0, 0, 0, 0)
SkibidiTitle.BackgroundColor3 = Color3.new(0, 0, 0)
SkibidiTitle.BorderColor3 = Color3.new(0, 0, 0)
SkibidiTitle.BorderSizePixel = 1
SkibidiTitle.Text = "Skibidi Bypass (Tutorial needed)"
SkibidiTitle.TextSize = 14
SkibidiTitle.TextColor3 = Color3.new(1, 1, 1)
SkibidiTitle.Font = Enum.Font.Bodoni
SkibidiTitle.Parent = SkibidiMain

-- Set up the text input box (for chat bypass input)
TextBax.Size = UDim2.new(0.8, 0, 0.2, 0)
TextBax.Position = UDim2.new(0.1, 0, 0.3, 0)
TextBax.BackgroundColor3 = Color3.new(0, 0, 0)
TextBax.BorderColor3 = Color3.new(0, 0, 0)
TextBax.BorderSizePixel = 1
TextBax.PlaceholderText = "(Chat)"
TextBax.TextColor3 = Color3.new(1, 1, 1)
TextBax.Font = Enum.Font.Code
TextBax.Text = ""
TextBax.TextSize = 15
TextBax.Parent = SkibidiMain

-- Set up the 'Chat' button
chat.Size = UDim2.new(0.3, 0, 0.2, 0)
chat.Position = UDim2.new(0.35, 0, 0.7, 0)
chat.BackgroundColor3 = Color3.new(0, 0, 0)
chat.BorderColor3 = Color3.new(0, 0, 0)
chat.BorderSizePixel = 1
chat.Text = "Chat"
chat.TextSize = 14
chat.TextColor3 = Color3.new(1, 1, 1)
chat.Font = Enum.Font.Code
chat.Parent = SkibidiMain

-- When the "Chat" button is clicked, run the Bypass function
chat.MouseButton1Click:Connect(function()
    local inputString = TextBax.Text
    -- Call the bypass function with input from the text box
    Bypass(inputString)  -- Assuming you have the Bypass function available in the same scope
end)

-- Optional: You can add more buttons or features as needed

-- Parent the GUI to the CoreGui (or another location) to display
gui.Parent = game:GetService("CoreGui")
