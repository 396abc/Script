local gui = Instance.new("ScreenGui")
local mainFrame = Instance.new("Frame")
local titleLabel = Instance.new("TextLabel")
local inputField = Instance.new("TextBox")
local sendButton = Instance.new("TextButton")
local closeButton = Instance.new("TextButton")

-- Set up the main frame with a cool, minimalistic style
gui.Name = "UniqueSkibidiGui"
gui.Parent = game:GetService("CoreGui")
mainFrame.Size = UDim2.new(0.4, 0, 0.4, 0)
mainFrame.Position = UDim2.new(0.3, 0, 0.3, 0)
mainFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 35) -- Dark background
mainFrame.BorderSizePixel = 0
mainFrame.BackgroundTransparency = 0.2
mainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
mainFrame.ClipsDescendants = true
mainFrame.Parent = gui

-- Set up the title with a more modern and slick font
titleLabel.Size = UDim2.new(1, 0, 0.2, 0)
titleLabel.Position = UDim2.new(0, 0, 0, 0)
titleLabel.BackgroundTransparency = 1
titleLabel.Text = "Skibidi Bypass"
titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text
titleLabel.TextSize = 18
titleLabel.Font = Enum.Font.GothamBold
titleLabel.TextStrokeTransparency = 0.6
titleLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0) -- Cool text outline
titleLabel.Parent = mainFrame

-- Set up the input field with a clean, modern look
inputField.Size = UDim2.new(0.85, 0, 0.2, 0)
inputField.Position = UDim2.new(0.075, 0, 0.3, 0)
inputField.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
inputField.BackgroundTransparency = 0.5
inputField.BorderSizePixel = 0
inputField.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text
inputField.Font = Enum.Font.Code
inputField.TextSize = 14
inputField.PlaceholderText = "Enter bypass text"
inputField.Text = ""
inputField.Parent = mainFrame

-- Set up the send button with a stylish look and hover effects
sendButton.Size = UDim2.new(0.4, 0, 0.2, 0)
sendButton.Position = UDim2.new(0.3, 0, 0.6, 0)
sendButton.BackgroundColor3 = Color3.fromRGB(0, 128, 255) -- Bright blue
sendButton.TextColor3 = Color3.fromRGB(255, 255, 255)
sendButton.Text = "Send"
sendButton.Font = Enum.Font.Gotham
sendButton.TextSize = 16
sendButton.TextStrokeTransparency = 0.5
sendButton.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
sendButton.BorderSizePixel = 0
sendButton.Parent = mainFrame

-- Hover effect for the send button
sendButton.MouseEnter:Connect(function()
    sendButton.BackgroundColor3 = Color3.fromRGB(0, 153, 255) -- Lighter blue on hover
end)

sendButton.MouseLeave:Connect(function()
    sendButton.BackgroundColor3 = Color3.fromRGB(0, 128, 255) -- Original blue
end)

-- Set up the close button with a minimal design
closeButton.Size = UDim2.new(0.1, 0, 0.1, 0)
closeButton.Position = UDim2.new(0.9, 0, 0, 0)
closeButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red close button
closeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
closeButton.Text = "X"
closeButton.Font = Enum.Font.GothamBold
closeButton.TextSize = 16
closeButton.TextStrokeTransparency = 0.6
closeButton.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
closeButton.BorderSizePixel = 0
closeButton.Parent = mainFrame

-- When the close button is clicked, remove the GUI
closeButton.MouseButton1Click:Connect(function()
    gui:Destroy()
end)

-- When the "Send" button is clicked, run the bypass function
sendButton.MouseButton1Click:Connect(function()
    local inputString = inputField.Text
    -- Assuming the Bypass function is available, you can run it with the input text
    Bypass(inputString)  -- Call your Bypass function here
end)

-- Optional: Add a fade-in effect on GUI launch
mainFrame.BackgroundTransparency = 1
local tweenService = game:GetService("TweenService")
local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)

local tween = tweenService:Create(mainFrame, tweenInfo, {BackgroundTransparency = 0.2})
tween:Play()
